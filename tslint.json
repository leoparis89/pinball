{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "no-var-requires": false,
    "no-require-imports": false,
    "no-empty": false,
    "no-console": false,
    "variable-name": false
  },

  "linterOptions": {
    "exclude": ["**/*.json"]
  },
  "rulesDirectory": []
}
